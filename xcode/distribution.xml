<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-gui-script minSpecVersion="2">
  <title>Flint</title>
  <license file="license.rtf"></license>
  <options customize="never" require-scripts="false" hostArchitectures="x86_64"/>
  <volume-check>
    <allowed-os-versions>
      <os-version min="10.9"/>
    </allowed-os-versions>
  </volume-check>
  <choices-outline>
    <line choice="default">
      <line choice="jp.oist.flint"/>
    </line>
  </choices-outline>
  <choice id="default"/>
  <choice id="jp.oist.flint" visible="false">
    <pkg-ref id="jp.oist.flint"/>
  </choice>
  <pkg-ref id="jp.oist.flint" version="0" onConclusion="none">flint-component.pkg</pkg-ref>
</installer-gui-script>
